<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="405.0" prefWidth="608.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="socialnetwork.controllers.ConversationMenuViewController">
   <children>
      <Label layoutX="382.0" layoutY="14.0" text="Your conversations">
         <font>
            <Font name="Century" size="18.0" />
         </font>
      </Label>
      <ListView fx:id="ConversationView" layoutX="328.0" layoutY="42.0" prefHeight="349.0" prefWidth="267.0" />
      <Button fx:id="continueConversation" layoutX="109.0" layoutY="126.0" mnemonicParsing="false" onAction="#continueConversation" text="Continue conversation!">
         <font>
            <Font name="Century" size="13.0" />
         </font>
      </Button>
      <Button fx:id="CreateNewConv" layoutX="127.0" layoutY="190.0" mnemonicParsing="false" onAction="#newConv" text="Start a new chat!">
         <font>
            <Font name="Century" size="13.0" />
         </font>
      </Button>
      <Button layoutX="14.0" layoutY="365.0" mnemonicParsing="false" onAction="#toHomepage" text="Back to homepage">
         <font>
            <Font name="Century" size="13.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
